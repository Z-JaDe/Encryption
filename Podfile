source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '10.0'
use_frameworks!
#Framework

install! 'cocoapods', deterministic_uuids: false, generate_multiple_pod_projects: true

target:Encryption do
	pod 'CryptoSwift', :git => 'https://github.com/krzyzanowskim/CryptoSwift.git'
	pod 'FunctionalSwift', :path => '../FunctionalSwift'
end


